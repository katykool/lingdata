        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a66">
                <ANNOTATION_VALUE>It will be today/ but a bit later.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a69">
                <ANNOTATION_VALUE>At first, let's play a word game.</ANNOTATION_VALUE>
