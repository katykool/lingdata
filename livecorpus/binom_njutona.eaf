        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a103" ANNOTATION_REF="a74">
                <ANNOTATION_VALUE>What we are talking about?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a104" ANNOTATION_REF="a75">
                <ANNOTATION_VALUE>Let's take some word.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a105" ANNOTATION_REF="a76">
                <ANNOTATION_VALUE>Let's take the word trushin.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a107" ANNOTATION_REF="a78">
                <ANNOTATION_VALUE>So, how many different words </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a108" ANNOTATION_REF="a79">
                <ANNOTATION_VALUE>is it possible to make</ANNOTATION_VALUE>
