        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a110" ANNOTATION_REF="a81">
                <ANNOTATION_VALUE>Well,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a111" ANNOTATION_REF="a82">
                <ANNOTATION_VALUE>we are not linguists,</ANNOTATION_VALUE>
